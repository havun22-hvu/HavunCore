{
    "name": "havun/core",
    "description": "Shared services for Havun projects - Mollie, Bunq, Gmail, Memorial Reference logic",
    "type": "library",
    "version": "0.2.0",
    "license": "proprietary",
    "authors": [
        {
            "name": "Henk van Velzen",
            "email": "havun22@example.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Havun\\Core\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Havun\\Core\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Havun\\Core\\HavunCoreServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
